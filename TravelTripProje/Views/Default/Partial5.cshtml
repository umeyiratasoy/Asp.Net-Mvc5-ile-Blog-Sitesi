@using TravelTripProje.Models.Siniflar
@model List<Blog>


<div class="box1-left">
    @foreach (var x in Model)
    {


        <div class="bot-img">
            <a href="/Blog/BlogDetay/@x.Id"><img src="@x.BlogImage" alt="" style="width:120px; height:65px;" /></a>
        </div>
        <div class="bot-desc1">
            <p>@x.Aciklama.Substring(0, 70)...</p>
        </div>
        <div class="clear"></div>
    }
</div>
